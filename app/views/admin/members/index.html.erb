<div>
  <div>
    <h4>会員一覧</h4>
  </div>

  <div class="table">
	  <div>
		  <div>会員ID</div>
		  <div>氏名</div>
		  <div>ユーザーネーム</div>
		  <div>ステータス</div>
	  </div>
	  <% @members.each do |member| %>
	  <div>
		  <div><%= member.id %></div>
		  <div><%= link_to member.name, admin_member_path(member.id) %></div>
		  <div><%= member.user_name %></div>
		  <div>
		    <% if member.is_delete %>
          <div>退会</div>
        <% else %>
          <div>有効</div>
        <% end %>
      </div>
	  </div>
    <% end %>
</div>