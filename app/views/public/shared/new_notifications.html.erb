<div class="my-5"><%= render 'public/shared/new_post_likes', post_likes: @post_likes %></div>
<div class="my-5"><%= render 'public/shared/new_post_comments', post_comments: @post_comments %></div>
<% if member_signed_in? %>
  <div class="my-5"><%= render 'public/shared/new_relationships', relationships: @member_relationships %></div>
<% else %>
  <div class="my-5"><%= render 'public/shared/new_relationships', relationships: @trainer_relationships %></div>
<% end %>
<% if trainer_signed_in? %>
  <div class="my-5"><%= render 'public/shared/new_subscriptions', subscriptions: @subscriptions %></div>
<% end %>